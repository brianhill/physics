Cell[TextData[{
 "All I did was rename the long side ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f7ee9de8-220b-48bd-bfbe-c71a3b579a34"],
 " instead of ",
 Cell[BoxData[
  FormBox["c", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "75820e78-b35a-49be-8178-b86f574a63f7"],
 ", and then the best way to remember the three trig functions  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["a", "c"], "=", "sin\[Theta]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "74e2ab84-6626-4cb3-908c-d54f269e1793"],
 ",  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["b", "c"], "=", "cos\[Theta]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e735f2ab-8858-4928-8349-d84e805330d0"],
 " and  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["a", "b"], "=", "tan\[Theta]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6c19d29c-81cb-456f-8966-906f0123e2b3"],
 ",  is\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{"r", " ", "sin", "\[VeryThinSpace]", "\[Theta]"}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,
  FontSize->18,ExpressionUUID->"f49212ed-1fbe-4aa2-98bc-65a7be8b521d"],
 StyleBox[",       ",
  FontSize->16],
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{"b", "=", 
     RowBox[{"r", " ", "cos", "\[VeryThinSpace]", "\[Theta]"}]}],
    FontSize->18], TraditionalForm]],
  FormatType->TraditionalForm,
  FontSize->16,ExpressionUUID->"a4c2b59d-8b8e-4cf3-8051-0ba9d98d694c"],
 StyleBox[",      and      ",
  FontSize->16],
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["a", "b"], "=", 
    RowBox[{"tan", "\[VeryThinSpace]", "\[Theta]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,
  FontSize->18,ExpressionUUID->"df90ff50-5abd-4fae-b584-dc86dc8124c2"],
 "\n\nbecause that is the way you most often will want to write use the \
formulas."
}], "Text",
